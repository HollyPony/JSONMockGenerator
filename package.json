{
  "name": "json-mock-generator",
  "version": "0.3.0",
  "description": "A customizable mock api",
  "keywords": [
    "api",
    "rest",
    "mock"
  ],
  "author": "HollyPony <arthur.agombart@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HollyPony/JSONMockGenerator.git"
  },
  "bugs": {
    "url": "https://github.com/HollyPony/JSONMockGenerator/issues"
  },
  "main": "dist",
  "engines": {
    "node": "8.2.1",
    "npm": "5.3.0"
  },
  "scripts": {
    "serve": "nodemon -w src --exec \"node src\"",
    "build": "node src -s -D -d dist",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src && mocha tests/index.spec.js",
    "test:heroku": "TARGET=https://jmgen.herokuapp.com/ mocha tests/index.spec.js"
  },
  "homepage": "https://github.com/HollyPony/JSONMockGenerator#readme",
  "dependencies": {
    "chakram": "1.5.0",
    "chance": "1.0.13",
    "eslint": "4.19.1",
    "mocha": "5.0.5",
    "morgan": "1.9.0",
    "nodemon": "1.17.3",
    "restify": "7.1.0"
  }
}
