{
  "name": "json-mock-generator",
  "version": "0.0.0",
  "description": "A customizable mock api",
  "keywords": [
    "api",
    "rest",
    "mock"
  ],
  "author": "HollyPony <arthur.agombart@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HollyPony/JSONMockGenerator.git"
  },
  "bugs": {
    "url": "https://github.com/HollyPony/JSONMockGenerator/issues"
  },
  "main": "dist",
  "scripts": {
    "serve": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "homepage": "https://github.com/HollyPony/JSONMockGenerator#readme",
  "dependencies": {
    "restify": "^4.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  }

}
